<!ELEMENT date_naiss (annee,mois,jour)>

<!ELEMENT trimestre (module)+>
<!ATTLIST trimestre
  num CDATA #REQUIRED>

<!ELEMENT prenom (#PCDATA)>

<!ELEMENT nom_F (#PCDATA)>

<!ELEMENT numero (#PCDATA)>

<!ELEMENT voie (#PCDATA)>
